// This program calculates the number of digits in a factorial!
#include<iostream>
#include<cmath>

using namespace std;

int main(){

//ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0); Not using beacuse of unrequired flush operation

// t test cases
int t; cin >> t; while(t--){
        cout << "Enter a non-negative integer: ";
        int n;
        float logsum = 0;
        cin >> n;
        for(int i = 2; i <= n; i++){
            logsum+=log10(i);
        }
        int nod = floor(logsum + 1);
        int arr[nod];
        cout << nod << "\n";
    }
    return 0;
}
